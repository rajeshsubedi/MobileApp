<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="loginmenu.Homepage"
             xmlns:pages="clr-namespace:loginmenu"
             
            NavigationPage.HasNavigationBar="false">
  <FlyoutPage.Flyout>
    <pages:HomepageFlyout x:Name="FlyoutPage" BackgroundColor="White" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:HomepageDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>